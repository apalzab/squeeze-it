<% content_for :head do %>
   <%= stylesheet_link_tag :squeezes -%>
<% end %>

<section class="container main">
  <section class="left col-md-4">
    <%= render 'shared/profile_box', user: current_user %>
    <%= render 'shared/most_voted_box' %>
  </section>

  <section class="squeezes-all col-md-8">
    <h2>Last Squeezes</h2>
    <% @last_squeezes.each do |squeeze| %>
      <% cache squeeze do %>
        <%= render squeeze %>
      <% end %>
    <% end %>
  </section>

  <section class="squeezes-all col-md-8">
    <h2>My Squeezes</h2>
    <% current_user.squeezes.each do |squeeze| %>
      <% cache squeeze do %>
        <%= render squeeze %>
      <% end %>
    <% end %>
  </section>
</section>

