 <section id="sign-in-up" class="container sign-in-up">
    <div class="row">
        <article class="col-lg-4 col-lg-offset-2">
          <header><h2>Inicia Sesi√≥n</h2></header>
            <%= form_for(:user, :url => session_path(:user)) do |f| %>
                <ul>
                    <li><%= email_field("user", "email", :placeholder => "email@example.com") %></li>
                    <li><%= f.password_field("password", :placeholder => "password") %></li>
                    <li><%= f.submit 'Sign in' %></li>
                    <!-- <li><%= f.label :remember_me %><%= f.check_box :remember_me %></li> -->
                    <!-- <li><%= link_to "Forgot your password?", new_password_path(:user) %></li> -->
                </ul>
            <% end %>
          <%= render "devise/shared/links" %>
        </article>
    </div>
</section>
